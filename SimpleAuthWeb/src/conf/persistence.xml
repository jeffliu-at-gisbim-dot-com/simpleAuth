<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="SimpleAuthWebPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.gisbim.simpleAuthWeb.persist.PriUser</class>
    <class>com.gisbim.simpleAuthWeb.persist.Role</class>
    <class>com.gisbim.simpleAuthWeb.persist.Para</class>
    <class>com.gisbim.simpleAuthWeb.persist.Fun</class>
    <class>com.gisbim.simpleAuthWeb.persist.DataAuth</class>
    <class>com.gisbim.simpleAuthWeb.persist.Duty</class>
    <class>com.gisbim.simpleAuthWeb.persist.FunAuth</class>
    <class>com.gisbim.simpleAuthWeb.persist.Service</class>
    <class>com.gisbim.simpleAuthWeb.persist.Org</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/BuildingWorld"/>
      <property name="javax.persistence.jdbc.user" value="postgres"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.password" value="qwerfdsa"/>
    </properties>
  </persistence-unit>
</persistence>
